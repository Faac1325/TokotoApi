<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fabian.arevalo.tokotoapi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TokotoApi">
        <activity
            android:name=".Vista.Historial"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Vista.Olvidar"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Vista.Inicio"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Vista.Descripcion"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Vista.Registro"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Vista.Splash_screen"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Vista.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>